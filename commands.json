{
        "Readout RGC Version":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "dumpsys package de.grennith.rgc.remotegpscontroller | grep versionName",
                        "FIELDNAME": "RGC_Version"
                }
        ],
        "Readout Pogodroid Version":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "dumpsys package com.mad.pogodroid | grep versionName",
                        "FIELDNAME": "POGODROID_Version"
                }
        ],
        "CUSTOM - MAD-ATV update rom":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "su -c 'mount -o remount,rw /system && /system/bin/curl -k -s https://raw.githubusercontent.com/Map-A-Droid/MAD-ATV/master/update_mad.sh >/system/bin/update_mad.sh && chmod +x /system/bin/update_mad.sh && /system/bin/update_mad.sh -i'"
                }
        ],
        "CUSTOM - MAD-ATV update pogodroid":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "su -c '/system/bin/update_mad.sh -d'"
                }
        ],
        "CUSTOM - MAD-ATV update RGC":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "su -c '/system/bin/update_mad.sh -r'"
                }
        ],
        "CUSTOM - MAD-ATV enable pingreboot":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "su -c 'touch /sdcard/pingreboot'"
                }
        ],
            "CUSTOM - Readout IP Adress of device":
        [
                {
                        "TYPE": "jobType.PASSTHROUGH",
                        "SYNTAX": "ifconfig wlan0 |grep 'inet addr' |cut -d ':' -f2 |cut -d ' ' -f1 && ifconfig eth0 |grep 'inet addr' |cut -d ':' -f2 |cut -d ' ' -f1"
                }
        ],
	"CUSTOM - REBOOT by Mad Jobs": 
        [
                {
                        "TYPE": "jobType.REBOOT",
                        "SYNTAX": "su -c reboot"
                }
        ]        
}
